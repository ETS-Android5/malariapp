<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:id="@+id/Grid">

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:id="@+id/Header"
        android:gravity="center_vertical|center_horizontal"
        android:layout_weight="0.05"
        android:background="#1e506c"
        android:visibility="gone">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="@string/selectTab"
            android:id="@+id/HeaderTitle"
            android:gravity="center_vertical|center_horizontal"
            android:background="#1e506c"
            android:textColor="#ffffff"
            android:textSize="24sp"
            android:visibility="gone" />

    </LinearLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:id="@+id/GeneralBody"
        android:layout_weight="0.83">

        <TabHost
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:id="@+id/tabHost">

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:orientation="vertical"
                android:gravity="center">

                <Spinner
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/tabSpinner"
                    android:spinnerMode="dropdown"
                    android:layoutMode="clipBounds"
                    android:backgroundTintMode="add"
                    android:gravity="right"
                    android:paddingLeft="2dip"
                    android:paddingTop="2dip"
                    android:paddingBottom="2dip"
                    android:paddingRight="20dip"
                    android:backgroundTint="#1e506c" />

                <TabWidget
                    android:id="@android:id/tabs"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:textAlignment="center"
                    android:tabStripEnabled="true"
                    android:nestedScrollingEnabled="true"
                    android:showDividers="beginning|end"
                    android:measureWithLargestChild="true"
                    android:background="@android:color/darker_gray"
                    android:visibility="gone"></TabWidget>

                <FrameLayout
                    android:id="@android:id/tabcontent"
                    android:layout_width="fill_parent"
                    android:layout_height="0dp"
                    android:layout_weight="0.4">

                    <LinearLayout
                        android:id="@+id/healthFacilityInfo"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:orientation="vertical" >

                        <ScrollView
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/ScrollHealth"
                            android:minHeight="20dip"
                            android:layout_weight="0.09">

                            <GridLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:rowCount="1"
                                android:columnCount="1"
                                android:id="@+id/healthContent" />
                        </ScrollView>

                        <GridLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:id="@+id/scoreHealth"
                            android:columnCount="2"
                            android:rowCount="1"
                            android:minHeight="20dip"
                            android:layout_weight="0.91"
                            android:background="#1e506c" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/profile"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:orientation="vertical">

                        <ScrollView
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/ScrollProfile"
                            android:layout_weight="0.09">

                            <GridLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:rowCount="1"
                                android:columnCount="1"
                                android:id="@+id/profileContent">

                            </GridLayout>
                        </ScrollView>

                        <GridLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:id="@+id/scoreProfile"
                            android:columnCount="2"
                            android:rowCount="1"
                            android:layout_weight="0.91"
                            android:background="#1e506c"></GridLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/c1Clinical"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:orientation="vertical">

                        <ScrollView
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/ScrollClinical1"
                            android:layout_weight="0.09">

                            <GridLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:rowCount="1"
                                android:columnCount="1"
                                android:id="@+id/c1ClinicalContent" />
                        </ScrollView>

                        <GridLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:id="@+id/scoreC1Clinical"
                            android:columnCount="2"
                            android:rowCount="1"
                            android:layout_weight="0.91"
                            android:background="#1e506c"></GridLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/c1RDT"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:orientation="vertical">

                        <ScrollView
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/ScrollRdt1"
                            android:layout_weight="0.09">

                            <GridLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:rowCount="1"
                                android:columnCount="1"
                                android:id="@+id/c1RDTContent" />
                        </ScrollView>

                        <GridLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:columnCount="2"
                            android:rowCount="1"
                            android:id="@+id/scoreC1RDT"
                            android:layout_weight="0.91"
                            android:background="#1e506c"></GridLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/c1Microcospy"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:orientation="vertical" >

                        <ScrollView
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/ScrollMicroscopy1"
                            android:layout_weight="0.09">

                            <GridLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:rowCount="1"
                                android:columnCount="1"
                                android:id="@+id/c1MicroscopyContent" />
                        </ScrollView>

                        <GridLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:id="@+id/scoreC1Microscopy"
                            android:rowCount="1"
                            android:columnCount="2"
                            android:layout_weight="0.91"
                            android:background="#1e506c"></GridLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/c2Clinical"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:orientation="vertical" >

                        <ScrollView
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/ScrollClinical2"
                            android:layout_weight="0.09">

                            <GridLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:rowCount="1"
                                android:columnCount="1"
                                android:id="@+id/c2ClinicalContent" />
                        </ScrollView>

                        <GridLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:columnCount="2"
                            android:rowCount="1"
                            android:id="@+id/scoreC2Clinical"
                            android:layout_weight="0.91"
                            android:background="#1e506c"></GridLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/c2RDT"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:orientation="vertical" >

                        <ScrollView
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/ScrollRdt2"
                            android:layout_weight="0.09">

                            <GridLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:rowCount="1"
                                android:columnCount="1"
                                android:id="@+id/c2RDTContent" />
                        </ScrollView>

                        <GridLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:columnCount="2"
                            android:rowCount="1"
                            android:id="@+id/scoreC2RDT"
                            android:layout_weight="0.91"
                            android:background="#1e506c"></GridLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/c2Microscopy"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:orientation="vertical" >

                        <ScrollView
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/ScrollMicroscopy2"
                            android:layout_weight="0.09">

                            <GridLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:rowCount="1"
                                android:columnCount="1"
                                android:id="@+id/c2MicroscopyContent" />
                        </ScrollView>

                        <GridLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:columnCount="2"
                            android:rowCount="1"
                            android:id="@+id/scoreC2Microscopy"
                            android:layout_weight="0.91"
                            android:background="#1e506c"></GridLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/c3Clinical"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:orientation="vertical" >

                        <ScrollView
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/ScrollClinical3"
                            android:layout_weight="0.09">

                            <GridLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:rowCount="1"
                                android:columnCount="1"
                                android:id="@+id/c3ClinicalContent" />
                        </ScrollView>

                        <GridLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:columnCount="2"
                            android:rowCount="1"
                            android:id="@+id/scoreC3Clinical"
                            android:layout_weight="0.91"
                            android:background="#1e506c"></GridLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/c3RDT"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:orientation="vertical" >

                        <ScrollView
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/ScrollRdt3"
                            android:layout_weight="0.09">

                            <GridLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:rowCount="1"
                                android:columnCount="1"
                                android:id="@+id/c3RDTContent" />
                        </ScrollView>

                        <GridLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:columnCount="2"
                            android:rowCount="1"
                            android:id="@+id/scoreC3RDT"
                            android:layout_weight="0.91"
                            android:background="#1e506c"></GridLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/c3Microscopy"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:orientation="vertical" >

                        <ScrollView
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/ScrollMicroscopy3"
                            android:layout_weight="0.09">

                            <GridLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:rowCount="1"
                                android:columnCount="1"
                                android:id="@+id/c3MicroscopyContent" />
                        </ScrollView>

                        <GridLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:columnCount="2"
                            android:rowCount="1"
                            android:id="@+id/scoreC3Microscopy"
                            android:layout_weight="0.91"
                            android:background="#1e506c"></GridLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/adherence"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:orientation="vertical" >

                        <ScrollView
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/scrollAdherence"
                            android:layout_weight="0.09" >

                            <GridLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:rowCount="1"
                                android:columnCount="1"
                                android:id="@+id/adherenceContent" />
                        </ScrollView>

                        <GridLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:columnCount="2"
                            android:rowCount="1"
                            android:id="@+id/scoreAdherence"
                            android:layout_weight="0.91"
                            android:background="#1e506c"
                            android:visibility="gone"></GridLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/feedback"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:orientation="vertical" >

                        <ScrollView
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/scrollFeedback"
                            android:layout_weight="0.09" >

                            <GridLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:rowCount="1"
                                android:columnCount="1"
                                android:id="@+id/feedbackContent" />
                        </ScrollView>

                        <GridLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:columnCount="2"
                            android:rowCount="1"
                            android:id="@+id/scoreFeedback"
                            android:layout_weight="0.91"
                            android:background="#1e506c"></GridLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/environmentMaterial"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:orientation="vertical" >

                        <ScrollView
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/ScrollEnvironment"
                            android:layout_weight="0.09">

                            <GridLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:rowCount="1"
                                android:columnCount="1"
                                android:id="@+id/environmentContent" />
                        </ScrollView>

                        <GridLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:columnCount="2"
                            android:rowCount="1"
                            android:id="@+id/scoreEnvironment"
                            android:layout_weight="0.91"
                            android:background="#1e506c"></GridLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/reporting"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:orientation="vertical" >

                        <ScrollView
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/scrollReporting"
                            android:layout_weight="0.09" >

                            <GridLayout
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:columnCount="1"
                                android:rowCount="1"
                                android:id="@+id/reportingContent"
                                android:layout_weight="0.91" />
                        </ScrollView>

                        <GridLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:columnCount="2"
                            android:rowCount="1"
                            android:id="@+id/scoreReporting"
                            android:layout_weight="0.91"
                            android:background="#1e506c"></GridLayout>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/iqaEQA"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:orientation="vertical" >

                        <ScrollView
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/scrollIqaEqa"
                            android:layout_weight="0.09" >

                            <GridLayout
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:columnCount="1"
                                android:rowCount="1"
                                android:id="@+id/IqaEqaContent"
                                android:layout_weight="0.91"></GridLayout>
                        </ScrollView>

                        <GridLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:columnCount="2"
                            android:rowCount="1"
                            android:id="@+id/scoreIqaEqa"
                            android:layout_weight="0.91"
                            android:background="#1e506c" />

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/scoreSummary"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:orientation="vertical"
                        android:visibility="gone">

                        <ScrollView
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/Scroll16"
                            android:layout_weight="0.09">

                            <GridLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/scoreContent"
                                android:columnCount="1"
                                android:rowCount="1">
                            </GridLayout>
                        </ScrollView>

                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/compositiveScores"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:orientation="vertical"
                        android:visibility="gone" >

                        <ScrollView
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/scrollCompositive"
                            android:layout_weight="0.09" >

                            <GridLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/compositiveContent"
                                android:columnCount="1"
                                android:rowCount="1" />
                        </ScrollView>
                    </LinearLayout>

                </FrameLayout>

            </LinearLayout>
        </TabHost>

    </LinearLayout>


    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:id="@+id/Footer"
        android:gravity="center_vertical|center_horizontal"
        android:layout_weight="0.12"
        android:weightSum="1"
        android:background="#429470">



        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/buttonsAndInfo"
            android:layout_weight="0.2">

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/save"
                android:id="@+id/save"
                android:onClick="saveClearResults" />

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/clear"
                android:id="@+id/clear"
                android:onClick="saveClearResults" />

        </LinearLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="@string/developers"
            android:id="@+id/textView"
            android:textColor="#ffffff"
            android:textStyle="bold|italic" />

    </LinearLayout>

</LinearLayout>